	               re-arrangement of the tree structure and minor cosmetic changes."				   		 
	
	REVISION      "200302041500Z"
	DESCRIPTION	  "Updated Common matrix mib information with minimal information."
	
	REVISION      "200212151200Z"
	DESCRIPTION	  "Added Common matrix mib information."
	
	REVISION      "200205211245Z"
	DESCRIPTION	  "First draft."            
	::= { gvgRouters 2 }
	
	--
	--  The Element MIB top-level groups
	--

	mtxNotifyGroup	OBJECT IDENTIFIER ::= { gvgRouters 4 }    

	-- Matrix System Information 
 	--
 	-- Sample System information
 	--
 	-- mtxSysName 		 : ConcertoAtNewsRoom
 	-- mtxSysDescr		 : Concerto at FIFA Live News Room
 	-- mtxFrameType		 : Concerto

 	mtxSysName 		OBJECT-TYPE
   		SYNTAX		OCTET STRING (SIZE(0..255))
  		MAX-ACCESS	read-only
  		STATUS		current
  		DESCRIPTION	"This object holds the name of Matrix - preferred to access/assign 
  					the same name as control system refers"
  	::= { matrices	1 }
  	
 	mtxSysDescr		OBJECT-TYPE
   		SYNTAX		OCTET STRING (SIZE(0..255))
  		MAX-ACCESS	read-only
  		STATUS		current
  		DESCRIPTION	"Describes more about the matrix capability and various signal types 
  					 the matrix can handle and gives the presence of matrix location"
  	::= { matrices	2 }
  	
 	mtxFrameType 	 OBJECT-TYPE
   		SYNTAX		 OCTET STRING (SIZE(0..64))
  		MAX-ACCESS	 read-only
  		STATUS		 current
  		DESCRIPTION	 "This object details the type of frame such as AES(NB), WB, Concerto etc.."
 	::= { matrices	3 }
 	
    mtxInputSignalStatus 	 OBJECT-TYPE
    	SYNTAX            	 INTEGER {
					     	  		  ok(1),
					    	  		  error(2),
				    	  		  	  missing(3),
									  notApplicable(4)
					  	     		 }
    	MAX-ACCESS         	   read-only
    	STATUS                 current
    	DESCRIPTION           "This attribute gives the status of all the configured input signals."
    ::= { matrices 4}             
    
    mtxOutputSignalStatus  	   OBJECT-TYPE
    	SYNTAX            	   INTEGER {
					     	  			ok(1),
					    	  			error(2),
					    	  		    missing(3),
									    notApplicable(4)
				  	    	 		   }
    	MAX-ACCESS         	   read-only
    	STATUS                 current
    	DESCRIPTION           "This attribute gives the status of all the configured output signals."
    ::= { matrices 5}             


    mtxBoardStatus   	 OBJECT-TYPE
    	SYNTAX           INTEGER {
					     		  ok(1),
					    		  error(2),
			    		  		  notApplicable(4)
				  		     	 }
    	MAX-ACCESS       read-only
    	STATUS           current
    	DESCRIPTION      "This attribute gives the status of all the Boards."
    ::= { matrices 6}             

    
    -- Frame Temperature
   
   	mtxFrameTemp  	  OBJECT-TYPE
   		SYNTAX        INTEGER {
   								ok(1),
   								error(2),
		    		  		    notApplicable(4)
   							  }
    	MAX-ACCESS    read-only
    	STATUS        current
    	DESCRIPTION   "This attribute give the Current temperature of the matrix frame"
    ::= { matrices 7 }

 	
	--	Power supply  
	-- =========================================================================================
	--  mtxPwrID	mtxPwrName	mtxPwrStatus	mtxPwrACStatus	mtxPwrDCStatus	mtxPwrFanStatus
	-- =========================================================================================
	--	1			PS1			Active			Present			Present			Present
	-- =========================================================================================
	--	2			PS2			missing			missing			missing			missing
	-- =========================================================================================
	--	3			PS3			faulty			missing			missing			missing     
	-- =========================================================================================
	
	mtxPowerSupplyTable 		OBJECT-TYPE
		SYNTAX	SEQUENCE OF 	MtxPowerSupplyEntry
		MAX-ACCESS 				not-accessible
		STATUS					current
		DESCRIPTION				"A table containing Power supply specific information"
	::= { matrices 8}
                        
	mtxPowerSupplyEntry 	OBJECT-TYPE
		SYNTAX				MtxPowerSupplyEntry  
		MAX-ACCESS			not-accessible
		STATUS				current
		DESCRIPTION			"Information about a particular Power Supply"
		INDEX				{mtxPwrID}
	::=	{  mtxPowerSupplyTable 1}   
		
	MtxPowerSupplyEntry	::= SEQUENCE{ mtxPwrID 			Integer32,
									  mtxPwrName		OCTET STRING,
							  		  mtxPwrStatus		INTEGER,									  
							  		  mtxPwrACStatus	INTEGER,
							  		  mtxPwrDCStatus 	INTEGER,
							  		  mtxPwrFanStatus 	INTEGER  }
		
    mtxPwrID	   	   OBJECT-TYPE
    	SYNTAX         Integer32 (1..2147483647)
    	MAX-ACCESS     read-only
    	STATUS         current
    	DESCRIPTION    "Sequence number or ID for the power supply present on frame"
    ::= { mtxPowerSupplyEntry 1}                                      
   
    mtxPwrName  	OBJECT-TYPE
   		SYNTAX     	OCTET STRING (SIZE(0..64))
    	MAX-ACCESS  read-only
    	STATUS      current
    	DESCRIPTION "Name of the power supply. For example PS1, PS2 etc.,"
    ::= { mtxPowerSupplyEntry 2}
    
  	mtxPwrStatus	 OBJECT-TYPE
   		SYNTAX     	 INTEGER {
					     	  active(1),
					    	  faulty(2),
					    	  missing(3),
				    		  notApplicable(4)
					  	     }
    	MAX-ACCESS  read-only
    	STATUS      current
    	DESCRIPTION "Gives the current status of power supply on its frame"
    ::= { mtxPowerSupplyEntry 3}
                                                    
  	mtxPwrACStatus		OBJECT-TYPE
   		SYNTAX       	INTEGER{
					    		 active(1),
				    			 faulty(2),
					    	 	 missing(3),
					    	 	 notApplicable(4) 
				  			   }
    	MAX-ACCESS   	read-only
    	STATUS       	current
    	DESCRIPTION  	"Gives the Current status of AC input on this power supply module.
    					 Not applicable case will be sent when there is no AC status is 
    					 supported by the matrix. In the case of Concerto we will not be
    					 able to find the AC input status to the power supply."    
    ::= { mtxPowerSupplyEntry 4}

  	mtxPwrDCStatus  	OBJECT-TYPE
   		SYNTAX       	INTEGER{
					    		 active(1),
					    		 faulty(2),
					    	 	 missing(3),
				    		 	 notApplicable(4) 
				    		   }
    	MAX-ACCESS   	read-only
    	STATUS       	current
    	DESCRIPTION  	"Gives the Current status of DC output from the power supply module.
    					 Not applicable case will be sent when there is no DC status is 
	    				 supported by the matrix. In the case of Concerto we will not be
    					 able to find the DC output status to the power supply."
    ::= { mtxPowerSupplyEntry 5}
                                                    
  	mtxPwrFanStatus 	OBJECT-TYPE
   		SYNTAX        	INTEGER{
					    	  	active(1),
					    	  	faulty(2),
					    	  	missing(3),
				    		  	notApplicable(4)
					  		 	}
    	MAX-ACCESS    	read-only
    	STATUS        	current
    	DESCRIPTION   	"Current status of Fan on this power supply module"
    ::= { mtxPowerSupplyEntry 6}
                                 
	-- Reference Signals 
	-- =================================================
	--  mtxRefIndex		mtxRefName  	mtxRefStatus                                    
	-- =================================================
	--	1				 VID			Present
	-- =================================================
	--	2				 AES1			Missing
	-- =================================================
	--	3				 AES2			Present                     
	-- =================================================
	--	4				 VREF1			Present                     
	-- =================================================
	--	5				 VREF2			Missing                     
	-- =================================================
	--	6				 TimeCode		Present                     
	-- =================================================

	mtxRefSignalTable 		OBJECT-TYPE
		SYNTAX	SEQUENCE OF MtxRefSignalEntry
		MAX-ACCESS 			not-accessible
		STATUS				current
		DESCRIPTION			"A table contains the details about the reference signal supported by the matrices."
	::= { matrices 9}
                        
	mtxRefSignalEntry 	  OBJECT-TYPE
		SYNTAX			  MtxRefSignalEntry
		MAX-ACCESS 		  not-accessible
		STATUS			  current
		DESCRIPTION		  "Information about a particular reference signal "
		INDEX			  {mtxRefIndex}
	::=	{  mtxRefSignalTable 1}   
		
	MtxRefSignalEntry		::= SEQUENCE{ mtxRefIndex	Integer32,   
									      mtxRefName	OCTET STRING,
							  	  	      mtxRefStatus	INTEGER}		
    mtxRefIndex	   	   OBJECT-TYPE
    	SYNTAX         Integer32 (1..2147483647)
    	MAX-ACCESS     read-only
    	STATUS         current
    	DESCRIPTION    "Reference signal Index."
    ::= { mtxRefSignalEntry 1}        
    
    mtxRefName	   	   OBJECT-TYPE
    	SYNTAX         OCTET STRING  (SIZE(0..64))
    	MAX-ACCESS     read-only
    	STATUS         current
    	DESCRIPTION    "Reference signal name - such as VID/AES1/AES2."
    ::= { mtxRefSignalEntry 2}

    mtxRefStatus   	   OBJECT-TYPE
    	SYNTAX         INTEGER {
    							present(1), 
    							faulty(2),
    							missing(3),
    							notApplicable(4)
	    					   }
    	MAX-ACCESS     read-only
    	STATUS         current
    	DESCRIPTION    "Reference signal status - such as Present/Not Present"
    ::= { mtxRefSignalEntry 3}          
    
    -- Fan Status                                   
	-- =================================================
    --  mtxFanId	mtxFanName	mtxFanStatus 
	-- =================================================
	--		1		Left Fan         Running
	-- =================================================
	--		2		Right Fan        Halted
	-- =================================================
	
	mtxFanStatusTable 		OBJECT-TYPE    
		SYNTAX	SEQUENCE OF MtxFanStatusEntry
		MAX-ACCESS 			not-accessible
		STATUS				current
		DESCRIPTION			"This table contains detailed information about each fan present in 
					 		 the matrix."
	::= { matrices 10}
                        
	mtxFanStatusEntry 	  OBJECT-TYPE
		SYNTAX			  MtxFanStatusEntry
		MAX-ACCESS 		  not-accessible
		STATUS			  current
		DESCRIPTION		  "Information about the status of a particular Fan."
		INDEX			  {mtxFanId }
	::=	{  mtxFanStatusTable 1}   
		
	MtxFanStatusEntry	::= SEQUENCE{ mtxFanId 	 	Integer32,
								  	  mtxFanName    OCTET STRING,
								  	  mtxFanStatus  INTEGER   }		
    mtxFanId 		OBJECT-TYPE
    	SYNTAX      Integer32 (1..2147483647)
    	MAX-ACCESS  read-only
    	STATUS      current
    	DESCRIPTION "Fan number"
    ::= { mtxFanStatusEntry 1}    
    
    mtxFanName		OBJECT-TYPE
    	SYNTAX      OCTET STRING  (SIZE(0..64))
    	MAX-ACCESS  read-only
    	STATUS      current
    	DESCRIPTION "This attribute gives the Name of the Fan such as PS1 , PS2"
    ::= { mtxFanStatusEntry 2}

   	mtxFanStatus   	   OBJECT-TYPE
   		SYNTAX         INTEGER{
					    		running(1),
					      		halted(2),
					      		missing(3),  
				    	  		notApplicable(4)
					  	      }
    	MAX-ACCESS     read-only
    	STATUS         current
    	DESCRIPTION    "Current status of Fan such as running or halted "
    ::= { mtxFanStatusEntry 3 }    
    
    mtxControllerStatus   	  OBJECT-TYPE
   		SYNTAX         		  INTEGER{
					    			active(1),
					      			inActive(2),
				    	  			unknown(3),  
					      			notSupported(4)
				  	    	  		}
    	MAX-ACCESS     read-only
    	STATUS         current
    	DESCRIPTION    "Current status of the matrix controller"
    ::= { matrices 21 }    
    
	-- Notification parameters used by Matrix                                  	
	
	msgClass		OBJECT-TYPE
    	SYNTAX      INTEGER{
        	                communication(1),
            	            equipment(2),
                	        environment(3),
                    	    processing(4)
		                  	}
    	MAX-ACCESS  read-only
    	STATUS      current
    	DESCRIPTION "This attribute gives the message class type of an alarm, suppose if an
       				 alarm is of type communication ,then it gives the message class as 
       				 communication "
    ::= { mtxNotifyGroup 1}        

    msgSeverity 	OBJECT-TYPE
    	SYNTAX      INTEGER{
        	                 major(1),
	                         minor(2),
	  						 warning(3),
    	                     critical(4),
        	                 information(5),
				 			 indeterminant(6),
			 				 cleared(7)
	    	                }
    	MAX-ACCESS  read-only
    	STATUS      current
    	DESCRIPTION "This attribute gives the message Severity of an alarm such as major/minor etc.,"
    ::= { mtxNotifyGroup 2}

    msgBehavior		OBJECT-TYPE
    	SYNTAX      INTEGER{        
							 clearedbyuser(1),
            	             clearedbysystem(2),
			    	     	 notpending(3)                                        
	                        }
    	MAX-ACCESS  read-only
   		STATUS      current
		DESCRIPTION "This attribute gives the message Behavior of an alarm."
    ::= { mtxNotifyGroup 3}

    faultDescriptionString   	 OBJECT-TYPE
    	SYNTAX             		 OCTET STRING  (SIZE(0..255))
    	MAX-ACCESS         		 read-only
    	STATUS                   current
    	DESCRIPTION              "This attribute gives the description of an alarm."
    ::= { mtxNotifyGroup 4}             

    
    -- Notification (Alarms) generated by a Matrix

	matrixPowerSupplyError		NOTIFICATION-TYPE
    	OBJECTS         
		{                      
			mtxSysName,
			mtxPwrID,
			mtxPwrName,
			mtxPwrStatus, 
			msgClass,
			msgSeverity,
			msgBehavior,
			faultDescriptionString
		}
    	STATUS          current
    	DESCRIPTION     "This alarm will be generated when  
						 there is an error in power supply module "
    ::= { matrices 11 }

	matrixPowerSupplyNoAC 		NOTIFICATION-TYPE
    	OBJECTS         
		{
			mtxSysName,
			mtxPwrID,
			mtxPwrName,		
			msgClass,
			msgSeverity,
			msgBehavior,
			faultDescriptionString		
		}
    	STATUS          current
    	DESCRIPTION     "This alarm will be generated when there is no AC input to the  
						 power supply module"
    ::= { matrices 12 }
                               
	matrixPowerSupplyNoDC 		NOTIFICATION-TYPE
    	OBJECTS         
		{
			mtxSysName,
			mtxPwrID,
			mtxPwrName,    		
			mtxPwrACStatus,		
			msgClass,
			msgSeverity,
			msgBehavior,
			faultDescriptionString
		}
    	STATUS          current
    	DESCRIPTION     "This alarm will be generated when there is a No DC
						 output from the power supply module"
    ::= { matrices 13 }
    
   	matrixPowerSupplyFanError	NOTIFICATION-TYPE
