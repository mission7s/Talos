	matrixTempOutofBound	NOTIFICATION-TYPE
    	OBJECTS         
		{
			mtxSysName,
			mtxFrameTemp,
			msgClass,
			msgSeverity,
			msgBehavior,
			faultDescriptionString		
		}
    	STATUS          current
