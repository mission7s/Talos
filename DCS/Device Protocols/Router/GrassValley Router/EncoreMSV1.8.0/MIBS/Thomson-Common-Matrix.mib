   	-- Extending gvgIOSignal Table for Matrix specific         
    -- ===============================================================
	--  mtxIndexNo	mtxConnNo	mtxIoSigDesc	mtxOutConnectedTo   
    -- ===============================================================
    --    1            1         AES Signal          -1
    -- ===============================================================
 	--    1            2         AES Signal		     -1
    -- ===============================================================
    --    1            3         SD Signal    	     -1
    -- ===============================================================
    --    1            1         SD Signal	          8
    -- ===============================================================
    --    2            2         HD Signal  	      6
    -- ===============================================================
	--    2            3         AES Signal  	      2
    -- ===============================================================
    
    --  1st example in above table describes - Logical matrix index no.1 and input connector no.1 contains AES signal 
    --  and last column contains -1, that is this row contains the Input signal information and this will not be contained
    --  any output connector number information
    
    -- Last example in above table describes - Logical matrix index no.2 and output connector no.3 contains AES signal 
    -- and last column contains 21, that is this row contains the Input signal connector number connected to output 
    -- connector no.2
           
           
	mtxIoSignalTable 	OBJECT-TYPE    
		SYNTAX	SEQUENCE OF MtxIoSignalEntry
		MAX-ACCESS 			not-accessible
		STATUS				current
		DESCRIPTION			"A list that contains detailed information about each Input/Output signal of the matrix."
	::= { matrices 31}
                        
	mtxIoSignalEntry   OBJECT-TYPE
		SYNTAX			  MtxIoSignalEntry
		MAX-ACCESS 		  not-accessible
		STATUS			  current
		DESCRIPTION		  "Additional objects for a gvgIoSignalEntry."
		AUGMENTS		 { gvgIoSignalEntry }
	::=	{  mtxIoSignalTable 1}   

	MtxIoSignalEntry	::= SEQUENCE{ mtxIndexNo 	 	Integer32,
									  mtxConnNo			Integer32,
									  mtxIoSigDesc    	OCTET STRING,
									  mtxOutConnectedTo	Integer32 }		
    
    mtxIndexNo 			OBJECT-TYPE
    	SYNTAX      	Integer32
    	MAX-ACCESS  	read-only
    	STATUS      	current
    	DESCRIPTION 	"This is an Matrix index number. Entire physical matrix is logically dived into logical matrix
    					 and referred as matrix index number. This is an 1 based value"
    ::= { mtxIoSignalEntry 1}    

    mtxConnNo 			OBJECT-TYPE
    	SYNTAX      	Integer32
    	MAX-ACCESS  	read-only
    	STATUS      	current
    	DESCRIPTION 	"Matrix connector number."
    ::= { mtxIoSignalEntry 2}    

     mtxIoSigDesc		OBJECT-TYPE
    	SYNTAX      	OCTET STRING  (SIZE(0..64))
    	MAX-ACCESS  	read-only
    	STATUS      	current
    	DESCRIPTION 	"This attribute gives the IO Signal description, such as AES Signal/SD Signal etc."
    ::= { mtxIoSignalEntry 3}        

    mtxOutConnectedTo 	OBJECT-TYPE
    	SYNTAX      	Integer32
    	MAX-ACCESS  	read-only
    	STATUS      	current
    	DESCRIPTION 	"Matrix Input connector number that is currently connected to output. 
    					 If the signal is in question is input signal , then this value will be -1."
    ::= { mtxIoSignalEntry 4}    

   
